/deep/ .mint-tab-item {
  display flex
  flex-direction column
  align-items center
  justify-content center
}

/deep/ .mint-header {
  background #FFFFFF
  font-size: 1rem;
  color: #333333;
  text-align: center;
}

/deep/ .mint-cell-text {
  font-size 1rem
  font-weight normal
  color #333333
}

/deep/ .mint-cell-value {
  font-size 0.85rem
  color #999999
}

/deep/ .mint-field-core::-webkit-input-placeholder{
  /* WebKit browsers */
  color: #CCCCCC;
  opacity 0.7
}

/deep/ .mint-field-core:-moz-placeholder{
  /* WebKit browsers */
  color: #CCCCCC;
  opacity 0.7
}

/deep/ .mint-field-core::-moz-placeholder{
  /* WebKit browsers */
  color: #CCCCCC;
  opacity 0.7
}

/deep/ .mint-field-core::-ms-input-placeholder{
  /* WebKit browsers */
  color: #CCCCCC;
  opacity 0.7
}
